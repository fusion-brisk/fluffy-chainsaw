{
  "name": "contentify-figma-plugin",
  "version": "2.4.1",
  "description": "Your Figma Plugin",
  "main": "code.js",
  "scripts": {
    "build": "rollup -c && node add-polyfill.js && node embed-ui.js",
    "build:watch": "rollup -c --watch",
    "build:code": "tsc src/code.ts --outDir dist --target es5 --moduleResolution node",
    "lint": "eslint --ext .ts,.tsx --ignore-pattern node_modules .",
    "lint:fix": "eslint --ext .ts,.tsx --ignore-pattern node_modules --fix .",
    "relay": "cd relay && npm start",
    "relay:install": "cd relay && npm install",
    "native-host:install": "cd native-host && npm install",
    "native-host:build": "cd native-host && npm run build",
    "dev": "concurrently -n build,relay -c blue,green \"npm run build:watch\" \"npm run relay\"",
    "dev:build": "npm run build:watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "author": "",
  "license": "",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/plugin-transform-async-to-generator": "^7.27.1",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@babel/preset-typescript": "^7.23.0",
    "@figma/eslint-plugin-figma-plugins": "*",
    "@figma/plugin-typings": "*",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.2.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "concurrently": "^9.2.1",
    "core-js": "^3.46.0",
    "crx3": "^2.0.0",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^27.2.0",
    "rollup": "^4.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.3.2",
    "vitest": "^4.0.16"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@figma/figma-plugins/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "root": true,
    "overrides": [
      {
        "files": [
          "*.js"
        ],
        "parser": "espree",
        "parserOptions": {
          "ecmaVersion": 2020,
          "sourceType": "script"
        },
        "rules": {
          "@typescript-eslint/no-unused-vars": "off"
        }
      }
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_",
          "caughtErrorsIgnorePattern": "^_"
        }
      ]
    }
  },
  "dependencies": {
    "canvas": "^3.2.1",
    "icojs": "^0.19.5",
    "papaparse": "^5.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
